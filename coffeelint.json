{
    "arrow_spacing": {
        "level": "warn"
    },
    "braces_spacing": {
        "value": 0,
        "level": "warn"
    },
    "colon_assignment_spacing": {
        "spacing": {
            "left": 0,
            "right": 1
        },
        "level": "warn"
    },
    "ensure_comprehensions": {
        "level": "ignore"
    },
    "eol_last": {
        "level": "warn"
    },
    "indentation": {
        "value": 4,
        "level": "warn"
    },
    "max_line_length": {
        "value": 79,
        "level": "warn"
    },
    "newlines_after_classes": {
        "value": 2,
        "level": "warn"
    },
    "no_backticks": {
        "level": "ignore"
    },
    "no_empty_param_list": {
        "level": "warn"
    },
    "no_trailing_semicolons": {
        "level": "error"
    },
    "no_trailing_whitespace": {
        "level": "warn"
    },
    "no_throwing_strings": {
        "level": "error"
    },
    "no_implicit_parens": {
        "level": "ignore"
    },
    "prefer_english_operator": {
        "level": "error"
    },
    "spacing_after_comma": {
        "level": "warn"
    },
    "camel_case_vars": {
        "module": "coffeelint-camel-case-vars",
        "level": "warn"
    },
    "no_code_after_return": {
        "module": "coffeelint-no-code-after-return"
    },
    "prefer_double_quotes": {
        "module": "coffeelint-prefer-double-quotes",
        "level": "error"
    },
    "variable_scope": {
        "module": "coffeelint-variable-scope",
        "scopeDiff": 1
    },
    "limit_newlines": {
        "module": "coffeelint-limit-newlines",
        "level": "warn",
        "value": 2
    },
    "use_strict": {
        "module": "coffeelint-use-strict",
        "level": "error",
        "allowGlobal": true,
        "requireGlobal": true
    }
}
